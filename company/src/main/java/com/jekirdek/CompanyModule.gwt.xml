<?xml version="1.0" encoding="UTF-8"?>
<!-- When updating your version of GWT, you should also update this DTD reference, so that your app can take advantage of 
	the latest GWT module capabilities. -->
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
  "http://www.gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module rename-to='company'>

	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.google.gwt.i18n.I18N" />

	<!-- Inherit the default GWT style sheet. You can change -->
	<!-- the theme of your GWT application by uncommenting -->
	<!-- any one of the following lines. -->
	<!-- <inherits name='com.google.gwt.user.theme.clean.Clean' /> -->
	<!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->

	<extend-property name="locale" values="tr" />
	<set-property name="locale" value="tr" />
	<set-property-fallback name="locale" value="tr" />

	<!-- Other module inherits -->

	<inherits name="org.gwtbootstrap3.GwtBootstrap3" />
	<inherits name="org.gwtbootstrap3.extras.select.Select" />
	<inherits name="org.gwtbootstrap3.extras.bootbox.Bootbox"/>
	<inherits name="org.gwtbootstrap3.extras.datetimepicker.DateTimePicker"/>
	<inherits name="org.gwtbootstrap3.extras.growl.Growl"/>

	<!-- Specify the app entry point class. -->
	<entry-point class='com.jekirdek.client.CompanyModule' />

	<!-- Specify the paths for translatable code -->
	<source path='client' />

</module>
